{
  "name": "Rugelach",
  "version": "0.0.1",
  "manifest_version": 1,
  "description": "because sometimes you just need a delicious Rugelach to bright up your page",
  "homepage_url": "https://github.com/avimak/Rugelach",
  "background": {
    "scripts": ["oven.js"],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["*://*.haaretz.co.il/*"]
  }],
  "permissions": [
    "*://*.haaretz.co.il/",
    "*://*.haaretz.com/"
  ]
}
